<div class='container'>
  <table class='table table-hover'>
    <thead>
      <tr>
        <th>Request Types</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>Most frequent request type</td>
        <td class="text-right"><%= @client.request_types.top %></td>
      </tr>
      <% @client.request_types.all_verbs.each do |k, v| %>
        <tr>
          <td><%= k %></td>
          <td class="text-right"><%= v %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
