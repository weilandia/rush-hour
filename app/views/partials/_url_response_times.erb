<table class='table table-hover'>
  <thead>
    <tr>
      <th>Response Times</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td>Average response time</td>
        <td class="text-right"><%= @url.avg_response_time %></td>
      </tr>
      <tr>
        <td>Max response time</td>
        <td class="text-right"><%= @url.max_response_time %></td>
      </tr>
      <tr>
        <td>Min response time</td>
        <td class="text-right"><%= @url.min_response_time %></td>
  </tbody>
</table>
<table class='table table-hover'>
  <thead>
    <tr>
      <th>Response Times Ordered by Frequency</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    </tr>
    <% @url.response_times_ordered.each do |response_time, count| %>
    <tr>
      <td><%= response_time %></td>
      <td class="text-right"><%= count %></td>
    </tr>
    <% end %>
  </tbody>
</table>
